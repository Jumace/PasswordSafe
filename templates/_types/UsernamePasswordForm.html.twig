{% extends 'PasswordSafeBundle:_tpl:typeForm.html.twig' %}
{% block content %}
  {{ parent() }}
  {% include 'PasswordSafeBundle:_elements:labelInput.html.twig' with {'name': 'username', 'label': 'Username', 'value': entry.username is defined ? entry.username : ''} %}
  {% include 'PasswordSafeBundle:_elements:labelInput.html.twig' with {'name': 'password', 'label': 'Password', type: 'password', 'value': entry.password is defined ? entry.password : ''} %}
  {% include 'PasswordSafeBundle:_elements:labelTextarea.html.twig' with {'name': 'note', 'label': 'Note', 'value': entry.note is defined ? entry.note : ''} %}
  <input type="hidden" name="type" value="UsernamePassword" />
{% endblock %}
