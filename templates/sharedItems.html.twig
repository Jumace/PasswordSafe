{% extends 'base.html.twig' %}

{% block main %}
    {% for user,items in sortedItems %}
        <h2 class="mdc-typograpy mdc-typography--headline">Shares from {{ user }}</h2>
        <ul class="mdc-list mdc-card entries">
            {% for row in items %}
                <li class="mdc-list-item">{% include '_tpl/type.html.twig'  with {'entry': row} %}</li>
            {% endfor %}
        </ul>
    {% endfor %}
{% endblock %}
